{
    "manifest_version": 2,
    "name": "Fila atendimento converja",
    "version": "1.0",
    "description": "Verifica ligações em fila",
    "permissions": [
        "activeTab",
        "notifications",
        "http://*/*",
        "tabs"
    ],

    "content_scripts": [
    {
        "matches": ["*://platform.converja.com/*"],
        "js": ["js/content.js"],
        "run_at": "document_end"
    }]
}